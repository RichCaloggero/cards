

{
"version": "0.2.0",
"configurations": [

{
"name": "Launch hearts",
"type": "firefox",
"request": "launch",
"reAttach": true,

    "log": {
        "consoleLevel": {
            "PathConversion": "Debug",
            "default": "Error"
        }
    },
"clearConsoleOnReload": true,
"file": "${workspaceFolder}/hearts.html",
"pathMappings": [{
"url": "http://localhost:3000/",
"path": "${workspaceFolder}"
},
],

}

]
}

